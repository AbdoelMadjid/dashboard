{% extends 'AppBundle:Layout:main.html.twig' %}

{% block content %}
    <div class="row">
        <div class="col-sm-6 col-md-3">
            <div class="chart-wrapper">
                <div id="block1" class="chart-stage"></div>
            </div>
        </div>
        <div class="col-sm-6 col-md-3">
            <div class="chart-wrapper">
                <div id="block2" class="chart-stage"></div>
            </div>
        </div>
        <div class="col-sm-6 col-md-3">
            <div class="chart-wrapper">
                <div id="block3" class="chart-stage"></div>
            </div>
        </div>
        <div class="col-sm-6 col-md-3">
            <div class="chart-wrapper">
                <div id="block4" class="chart-stage"></div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-6 col-md-8">
            <div class="chart-wrapper">
                <div id="block5" class="chart-stage"></div>
            </div>
        </div>
        <div class="col-sm-6 col-md-4">
            <div class="chart-wrapper">
                <div class="chart-stage">
                    <div id="block6"></div>
                    <div class="row">
                        <div id="paging" class="padding-7"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-6 col-md-3">
